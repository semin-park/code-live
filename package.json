{
  "name": "code-live",
  "version": "0.0.1",
  "description": "CodeLive for SWE-2020",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swe-cola/code-live.git"
  },
  "keywords": [
    "code",
    "editing",
    "live",
    "concurrent",
    "crdt"
  ],
  "author": "Team Cola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/swe-cola/code-live/issues"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "typescript": "^4.0.3",
    "yorkie-js-sdk": "0.0.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
